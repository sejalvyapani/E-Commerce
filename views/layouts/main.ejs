<!doctype html>
<html is="dmx-app">

<head>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.14.0/css/all.css" integrity="sha384-HzLeBuhoNPvSl5KYnjx0BT+WB0QEEqLprO+NBkkk5gbc67FTaL7XIGa2w1L0Xbgc" crossorigin="anonymous" />
  <link rel="stylesheet" href="css/bootstrap-icons.css" />
  <link rel="stylesheet" href="bootstrap/5/css/bootstrap.min.css" />
  <script src="dmxAppConnect/dmxAppConnect.js"></script>
  <script src="dmxAppConnect/dmxRouting/dmxRouting.js" defer=""></script>
  <script src="dmxAppConnect/dmxBootstrap5Navigation/dmxBootstrap5Navigation.js" defer=""></script>
  <script src="dmxAppConnect/dmxBootstrap5Offcanvas/dmxBootstrap5Offcanvas.js" defer=""></script>
  <script src="dmxAppConnect/dmxSwiper/dmxSwiper.js" defer=""></script>
  <script src="dmxAppConnect/dmxSwiper/swiper.min.js" defer=""></script>
  <link rel="stylesheet" href="dmxAppConnect/dmxSwiper/themes/default.css" />
  <script src="dmxAppConnect/dmxSmoothScroll/dmxSmoothScroll.js" defer=""></script>
  <script src="dmxAppConnect/dmxDataTraversal/dmxDataTraversal.js" defer=""></script>
  <base href="/">
  <meta charset="UTF-8">
  <title>Untitled Document</title>

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&family=Rubik:ital,wght@0,400;0,500;0,600;1,300;1,400;1,500&display=swap" rel="stylesheet">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="stylesheet" href="css/style.css" />
</head>

<body id="main">
  <dmx-json-datasource id="jsonDS1" is="dmx-serverconnect" url="assets/json/data.json"></dmx-json-datasource>

  <div class="offcanvas offcanvas-start w-75" id="offcanvasMenu" is="dmx-bs5-offcanvas" tabindex="-1" scroll="true">
    <div class="offcanvas-header">
      <h5 class="offcanvas-title"></h5>
      <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
      <h5 class="d-flex justify-content-between fw-bold mb-4">New Releases <img src="assets/images/arrow.svg"></h5>
      <h5 class="d-flex justify-content-between fw-bold mb-4">Men <img src="assets/images/arrow.svg"></h5>
      <h5 class="d-flex justify-content-between fw-bold mb-4">Women <img src="assets/images/arrow.svg"></h5>
      <h5 class="d-flex justify-content-between fw-bold mb-4">kids <img src="assets/images/arrow.svg"></h5>
      <h5 class="d-flex justify-content-between fw-bold mb-4">Sales <img src="assets/images/arrow.svg"></h5>
      <h5 class="d-flex justify-content-between fw-bold mb-4">Collections <img src="assets/images/arrow.svg"></h5>
      <button class="btn btn-outline-dark mt-5 rounded-0 w-100 p-2" type="button"> <img src="assets/images/location.svg" class="me-2">Store Locator
      </button>
      <p class="mt-4 font-large14"> <img src="assets/images/Accountsm.svg">ACCOUNT</p>
      <p class="font-large14"><img src="assets/images/Help.svg">HELP</p>
      <p class="font-large14"><img src="assets/images/Newsletter.svg">NEWSLETTER</p>
    </div>
  </div>
  <div class="d-flex container-fluid justify-content-center bg-dark font-medium12 text-white">
    <p class=" mb-0 py-2">
      20% Off! Code: SPRING@!-Terms apply*
    </p>
  </div>
  <div class="  d-flex container justify-content-between p-0 mt-5">
    <div class="col-lg-4 col-md-4 ">
      <img src="assets/images/Logo-lockup.svg" class="me-5 ms-2">
    </div>
    <div class="col-lg-4 col-md-6 d-flex justify-content-end align-items-center">
      <span class="d-none me-5 font-medium12 d-lg-inline">
        <img src="assets/images/support.svg">
        Support </span>
      <span class="font-medium12"><img src="assets/images/location.svg"><span class="d-none d-lg-inline">Find a store</span><span class="d-lg-none font-medium12 d-flex align-items-center me-3">Store</span></span>
    </div>
    <div class="col-lg-4 col-md-2 d-flex justify-content-lg-end align-items-center ">
      <img src="assets/images/Heart.svg" class="account-icon d-none d-lg-inline">
      <span> <img src="assets/images/Account.svg" class="me-2 ms-2 account-icon"><span class="d-lg-none font-medium12  d-flex align-items-center me-3">Account</span></span>
      <span> <img src="assets/images/Cart.svg" class="me-2 account-icon"><span class="d-lg-none font-medium12  d-flex align-items-center me-3">Cart</span></span>
    </div>
  </div>
  <div class="container mt-5 p-0">
    <div class="border-bottom mb-3 ">
      <div class="d-none d-lg-inline">
        <span class="fw-bold me-2 font-large14">WOMEN</span>
        <span class="me-2 font-large14">MEN</span>
        <span class="font-large14">KIDS</span>
      </div>
    </div>
    <nav class="navbar navbar-expand-lg navbar-light">
      <div class="d-flex w-100 justify-content-between">
        <div>
          <button class="bg-white font-large14  border-0 navbar-toggler d-lg-none  align-items-center d-flex text-dark " type="button" dmx-on:click="offcanvasMenu.toggle()">
            <span class="navbar-toggler-icon"></span> <span class="ms-2">MENU</span>
          </button>
          <div class="d-none d-lg-inline">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0 font-medium12" is="dmx-repeat" id="repeat1" dmx-bind:repeat="jsonDS1.data.list">
              <li class="nav-item">
                <a class="nav-link  text-dark" href="#" dmx-class:ps-0="$index==0">{{title}}</a>
              </li>

            </ul>
          </div>
        </div>
        <div>
          <form class="d-flex form-border">
            <img src="assets/images/Search.svg" class="ms-1 d-none d-lg-inline">
            <input class=" w-auto form-control border-0 rounded-0 font-large14 me-2" type="search" placeholder="Search products,articles,faq,..." aria-label="Search">
            <img src="assets/images/Search.svg" class=" d-lg-none  me-3">
            <img src="assets/images/Microphone.svg" class="ms-1 d-none d-lg-inline">
            <img src="assets/images/Camera.svg" class="border-start mb-1 ms-1 mt-1 px-2 d-none d-lg-inline">
          </form>
        </div>
      </div>
    </nav>
  </div>
  <div is=" dmx-view" id="content">
    <%- await include(content, locals); %>
  </div>
  <div class="bg-light">
    <div class="d-flex container flex-column flex-lg-row pb-5  mt-5 ">
      <div class="col-lg-2 ps-5">
        <ul class="pt-5 list-unstyled ">
          <h5>FOOTER1</h5>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
        </ul>
      </div>
      <div class="col-lg-2 ps-5">
        <ul class="pt-5 list-unstyled">
          <h5>FOOTER2</h5>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
        </ul>
      </div>
      <div class="col-lg-2 ps-5">
        <ul class="pt-5 list-unstyled">
          <h5>FOOTER3</h5>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
        </ul>
      </div>
      <div class="col-lg-2 ps-5">
        <ul class="pt-5 list-unstyled">
          <h5>FOOTER4</h5>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
        </ul>
      </div>
      <div class="col-lg-2 ps-5">
        <ul class="pt-5 list-unstyled">
          <h5>FOOTER5</h5>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
        </ul>
      </div>
      <div class="col-lg-2 ps-5">
        <ul class="pt-5 list-unstyled">
          <h5>FOOTER6</h5>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
          <li class="text-secondary">Item</li>
        </ul>
      </div>
    </div>
  </div>
  <div class="d-flex container-fluid justify-content-center bg-dark  text-white">
    <img src="assets/images/Symbol.svg" class="mt-3 mb-3">
  </div>
  <script src="bootstrap/5/js/bootstrap.bundle.min.js"></script>
</body>

</html>